#include <stdio.h>
#include <limits.h>
#define V 5

int minKey(int key[], int used[]) {
    int min = INT_MAX, idx = -1;
    for (int i = 0; i < V; i++)
        if (!used[i] && key[i] < min)
            min = key[i], idx = i;
    return idx;
}

void primMST(int g[V][V]) {
    int parent[V], key[V], used[V] = {0};
    for (int i = 0; i < V; i++) key[i] = INT_MAX;
    key[0] = 0; parent[0] = -1;

    for (int e = 0; e < V - 1; e++) {
        int u = minKey(key, used);
        used[u] = 1;
        for (int v = 0; v < V; v++)
            if (g[u][v] && !used[v] && g[u][v] < key[v])
                parent[v] = u, key[v] = g[u][v];
    }

    printf("Edge  Weight\n");
    for (int i = 1; i < V; i++)
        printf("%d - %d    %d\n", parent[i], i, g[i][parent[i]]);
}

int main() {
    int graph[V][V] = {
        {0, 2, 0, 6, 0},
        {2, 0, 3, 8, 5},
        {0, 3, 0, 0, 7},
        {6, 8, 0, 0, 9},
        {0, 5, 7, 9, 0}
    };
    primMST(graph);
    return 0;
}
